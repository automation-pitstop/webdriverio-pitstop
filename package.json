    "private": true,
    "devDependencies": {
        "@babel/core": "^7.17.8",
        "@babel/preset-env": "^7.16.11",
        "@babel/register": "^7.17.7",
        "@wdio/allure-reporter": "^7.19.5",
        "@wdio/cli": "^7.19.2",
        "@wdio/junit-reporter": "^7.19.1",
        "@wdio/local-runner": "^7.19.2",
        "@wdio/mocha-framework": "^7.19.2",
        "@wdio/spec-reporter": "^7.19.1",
        "allure-commandline": "^2.17.2",
        "chromedriver": "^99.0.0",
        "csv": "^6.0.5",
